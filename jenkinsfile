pipeline {
    agent {
        node {
            label 'Built-In Node'
        }
    }

    stage('Code checkout') {
        steps {
            git 'https://github.com/atilanof/PruebaJenkins.git'
        }
    }
    stage('Build') {
        steps {
            sh '"mvn" -Dmaven.test.failure.ignore clean install'
        }
    }


}